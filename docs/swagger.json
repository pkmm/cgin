    "definitions": {
        "co.AuthCredit": {
            "type": "object",
            "properties": {
                "token": {
                    "type": "string",
                    "example": "tokne_example.asdass"
                }
            }
        },
        "co.AuthModel": {
            "type": "object",
            "properties": {
                "device_type": {
                    "type": "integer",
                    "example": 1
                },
                "openid": {
                    "type": "string",
                    "example": "aqe"
                },
                "sign": {
                    "type": "string",
                    "example": "ewe"
                },
                "token": {
                    "type": "string",
                    "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1aWQiOjgsImV4cCI6MTU2MjQ2NTY0MSwiaXNzIjoiY2NsYSJ9.J3QXF3tZlvzyWvx8VG9EibUIr5mHK0xg3mjxY8LGhk8"
                }
            }
        },
        "co.PageLimitOffset": {
            "type": "object",
            "properties": {
                "page": {
                    "type": "integer",
                    "example": 1
                },
                "size": {
                    "type": "integer",
                    "example": 100
                }
            }
        },
        "co.TaskName": {
            "type": "object",
            "properties": {
                "job_name": {
                    "type": "string",
                    "example": "sign_baidu_tieba"
                }
            }
        },
        "service.Response": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "data": {
                    "type": "object"
                },
                "msg": {
                    "type": "string"
                }
            }
        }
    }
}